{
	"name": "To Do List",
    "manifest_version": 2,
	"version": "1.0",
	"description": "Organize your tasks for the day!",
	"permissions": ["tabs"],
	"icons": {
		"32": "icons/bullet-points32.png"
	},
	"browser_action": {
		"default_icon": "icons/bullet-points32.png"
	},
	"background": {
		"scripts": ["scripts/chromeBackground.js"],
		"persistant": false
	}


}